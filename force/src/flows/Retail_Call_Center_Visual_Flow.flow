        <locationX>678</locationX>
        <locationY>162</locationY>
        <connector>
            <targetReference>Costco_CVS_Affiliate_OON_Sub_Flow</targetReference>
        </connector>
        <inputAssignments>
            <field>Flow_Name__c</field>
