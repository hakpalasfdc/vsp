<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <choices>
        <name>CostcoCignaVisionMemberChoice</name>
        <choiceText>Costco for CIGNA Vision Member</choiceText>
        <dataType>String</dataType>
    </choices>
    <choices>
        <name>CostcoCVSAffiliateServicesChoice</name>
        <choiceText>Costco, CVS or Affiliate (AFPR, AFPRQ) or Out-of-Network</choiceText>
        <dataType>String</dataType>
    </choices>
    <choices>
        <name>MemberReceivingServicesChoice</name>
        <choiceText>Member/Patient (not a Cigna Vision member) receiving services at Costco, CVS or Affiliate (AFPR, AFPRQ) or Out-of-Network</choiceText>
        <dataType>String</dataType>
    </choices>
    <choices>
        <name>No</name>
        <choiceText>No</choiceText>
        <dataType>String</dataType>
        <value>
            <stringValue>No</stringValue>
        </value>
    </choices>
    <choices>
        <name>WalmartSamsVisionWorksServicesChoice</name>
        <choiceText>Walmart/Sam&apos;s Club Optical or Vision Works</choiceText>
        <dataType>String</dataType>
    </choices>
    <choices>
        <name>Yes</name>
        <choiceText>Yes</choiceText>
        <dataType>String</dataType>
        <value>
            <stringValue>Yes</stringValue>
        </value>
    </choices>
    <constants>
        <description>Flow Name</description>
        <name>FlowName</name>
        <dataType>String</dataType>
        <value>
            <stringValue>Retail Call Center Visual Flow</stringValue>
        </value>
    </constants>
    <decisions>
        <name>Who_is_your_caller_decision</name>
        <label>Who is your caller decision?</label>
        <locationX>344</locationX>
        <locationY>50</locationY>
        <defaultConnector>
            <targetReference>User_Flow_Tracking_Record</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>CostcoCVSAffiliateOONDecision</defaultConnectorLabel>
        <rules>
            <name>MemberPatientDecision</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>MemberReceivingServicesChoice</leftValueReference>
                <operator>WasSelected</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>User_Flow_Tracking_Record_3</targetReference>
            </connector>
            <label>MemberPatientDecision</label>
        </rules>
        <rules>
            <name>WalmartVWDecision</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>WalmartSamsVisionWorksServicesChoice</leftValueReference>
                <operator>WasSelected</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>User_Flow_Tracking_Record_2</targetReference>
            </connector>
            <label>WalmartVWDecision</label>
        </rules>
    </decisions>
    <description>Visual flow to guide users through a call so that they can best serve a customer</description>
    <formulas>
        <name>Today</name>
        <dataType>Date</dataType>
        <expression>TODAY()</expression>
    </formulas>
    <formulas>
        <name>UserId</name>
        <dataType>String</dataType>
        <expression>{!$User.Id}</expression>
    </formulas>
    <interviewLabel>Retail Flow {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Retail Call Center Visual Flow</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>Flow</processType>
    <recordCreates>
