    <validationRules>
        <fullName>CommitteeStatus_and_CommitteeDate_Reqd</fullName>
        <active>true</active>
        <description>Committee Status &amp; Date Required when Credential Status = Committee</description>
        <errorConditionFormula>AND(
ISPICKVAL( Credentialing_Status__c, &quot;Committee&quot;),
OR(
ISPICKVAL( Committee_Status__c, &quot;&quot; ),
ISBLANK( Committee_Date__c ))
)</errorConditionFormula>
        <errorMessage>Committee Date and Committee Status must be complete</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CredentialStatus_and_CommitteeDate_Reqd</fullName>
        <active>true</active>
        <description>Credential Status &amp; Committee Date Required when Committee Status = Recommend Denial</description>
        <errorConditionFormula>AND(
ISPICKVAL( Committee_Status__c, &quot;Recommend Denial&quot;),
OR(
ISPICKVAL( Credentialing_Status__c, &quot;&quot; ),
ISBLANK( Committee_Date__c ))
)</errorConditionFormula>
        <errorMessage>Credentialing Status and Committee Date must be complete.</errorMessage>
    </validationRules>
