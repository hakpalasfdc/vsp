    <validationRules>
        <fullName>Malformed_state_code</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
LEN(State__c) &lt;&gt; 2 &amp;&amp;
NOT(
CONTAINS(&quot;AL:AK:AZ:AR:CA:CO:CT:DE:DC:FL:GA:HI:ID:&quot; &amp;
&quot;IL:IN:IA:KS:KY:LA:ME:MD:MA:MI:MN:MS:MO:MT:NE:NV:NH:&quot; &amp;
&quot;NJ:NM:NY:NC:ND:OH:OK:OR:PA:RI:SC:SD:TN:TX:UT:VT:VA:&quot; &amp;
&quot;WA:WV:WI:WY:PR:AB:BC:MB:NB:NL:NT:NS:NU:ON:PC:QC:SK:YT&quot;, State__c)
),
$Profile.Name &lt;&gt; &quot;VSP Technical User&quot;)</errorConditionFormula>
        <errorMessage>Please enter a valid 2 digit US State or Canadian code</errorMessage>
    </validationRules>
