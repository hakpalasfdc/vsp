    <validationRules>
        <fullName>Premier_WhenBenefitTypeIsCheckbox</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( Benefit_Type__c , &apos;Checkbox&apos;) 

&amp;&amp; 

(Benefit_Checkbox__c = false

|| 

NOT(ISBLANK(Benefit_Amount__c))

|| 

NOT(ISBLANK(Benefit_Discount__c)))

&amp;&amp; 

NOT(OR( 
$Profile.Name = &quot;System Administrator&quot;, 
$Profile.Name = &quot;VSP Technical User&quot;) 
)</errorConditionFormula>
        <errorMessage>You must check Benefit Checkbox. Benefit Amount and Benefit Discount is not allowed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Premier_WhenBenefitTypeIsCurrency</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( Benefit_Type__c , &apos;Currency&apos;)

&amp;&amp; 

(ISBLANK(Benefit_Amount__c) 

||

(NOT(ISBLANK( Benefit_Discount__c)) 

||
 
Benefit_Checkbox__c = true))

&amp;&amp; 

NOT(OR( 
$Profile.Name = &quot;System Administrator&quot;, 
$Profile.Name = &quot;VSP Technical User&quot;) 
)</errorConditionFormula>
        <errorMessage>You must enter a Benefit Amount. Benefit Discount and Benefit Checkbox is not allowed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Premier_WhenBenefitTypeIsDiscount</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( Benefit_Type__c , &apos;Discount&apos;) 

&amp;&amp; 

(ISBLANK(Benefit_Discount__c) 

|| 

NOT(ISBLANK(Benefit_Amount__c)) 

|| 

Benefit_Checkbox__c = true)

&amp;&amp; 

NOT(OR( 
$Profile.Name = &quot;System Administrator&quot;, 
$Profile.Name = &quot;VSP Technical User&quot;) 
)</errorConditionFormula>
        <errorMessage>You must enter a Benefit Discount. Benefit Amount and Benefit Checkbox is not allowed.</errorMessage>
    </validationRules>
