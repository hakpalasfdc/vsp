    <validationRules>
        <fullName>VSPR19b_Claims_Priced_Restriction</fullName>
        <active>true</active>
        <description>No changes can be made once claims have been priced against this Network Relationship.</description>
        <errorConditionFormula>AND(
ISPICKVAL(Claims_Priced__c , &quot;Y&quot;),
OR(
ISCHANGED (Contact_Role__c),
ISCHANGED (Account_Name__c),
ISCHANGED (Provider_Name__c), 
ISCHANGED (Program_ID__c ),
ISCHANGED (Program_Effective_Date__c),
ISCHANGED (LVC_Negotiated_Discount__c),
ISCHANGED (Fee_Code__c))
)

&amp;&amp;
Bypass_VC_Validation_Rules__c = false</errorConditionFormula>
        <errorMessage>Claims have been priced against this Network Relationship. You cannot make any changes.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VSPR19b_Negotiated_Discount_0_To_100</fullName>
        <active>true</active>
        <description>LVC Negotiated Discount must be between 0 and 100. Ensures non-numeric text and values outside the range are not saved.</description>
        <errorConditionFormula>OR(LVC_Negotiated_Discount__c &lt; 0.00, LVC_Negotiated_Discount__c &gt; 1.00)
&amp;&amp;
NOT( $Profile.Name = &quot;System Administrator&quot; || $Profile.Name = &quot;VSP Technical User&quot;)

&amp;&amp;
Bypass_VC_Validation_Rules__c = false</errorConditionFormula>
        <errorMessage>You must enter an LVC Negotiated Discount of 0-100</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VSPR19b_Negotiated_Discount_Required</fullName>
        <active>true</active>
        <description>Require user to enter an LVC Negotiated Discount for Program ID LVC01</description>
        <errorConditionFormula>AND(
IF(
    AND(
        Program_ID__c = &quot;LVC01&quot;, 
        ISBLANK(LVC_Negotiated_Discount__c )), True, False
 )
,
NOT( $Profile.Name = &quot;System Administrator&quot; || $Profile.Name = &quot;VSP Technical User&quot;)
)

&amp;&amp;
Bypass_VC_Validation_Rules__c = false</errorConditionFormula>
        <errorMessage>You must enter an LVC Negotiated Discount for this Program ID</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VSPR19b_Program_Effective_Date_Account</fullName>
        <active>true</active>
        <description>Program ID Effective Date cannot be less than the Account Vision Care Effective Date.</description>
        <errorConditionFormula>AND(
ISNEW()||ISCHANGED(Program_Effective_Date__c),
Program_Effective_Date__c &lt; Contact_Role__r.Account__r.Vision_Care_Effective_Date__c,
$Profile.Name &lt;&gt; &quot;System Administrator&quot;,
$Profile.Name &lt;&gt; &quot;VSP Technical User&quot;,
$Profile.Name &lt;&gt; &quot;VSP Integration User&quot;,
