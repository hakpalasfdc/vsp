    <fields>
        <fullName>Exception_Reason_Calculation__c</fullName>
        <description>Calculates the Vision Care Exception Reasons from Practice Account, Business Account, Contact, and Contact Role and displays the exception if the Program ID = VSP</description>
        <externalId>false</externalId>
        <formula>IF( Program_ID__r.Name  = &apos;VSP&apos; &amp;&amp; NOT(ISBLANK( TEXT(Account_Name__r.Parent.Vision_Care_Practice_Exception__c ))), TEXT(Account_Name__r.Parent.Vision_Care_Practice_Exception__c), 

IF(Program_ID__r.Name = &apos;VSP&apos; &amp;&amp; NOT(ISBLANK(TEXT( Account_Name__r.Vision_Care_Location_Exception__c ))), TEXT( Account_Name__r.Vision_Care_Location_Exception__c),

IF(Program_ID__r.Name = &apos;VSP&apos; &amp;&amp; NOT(ISBLANK(TEXT( Provider_Name__r.Vision_Care_Doctor_Exception__c ))), TEXT( Provider_Name__r.Vision_Care_Doctor_Exception__c), 

IF(Program_ID__r.Name = &apos;VSP&apos; &amp;&amp; NOT(ISBLANK(TEXT( Contact_Role__r.Vision_Care_Contact_Role_Exception__c ))), TEXT( Contact_Role__r.Vision_Care_Contact_Role_Exception__c), &quot;&quot;)
)
)
)</formula>
        <label>Exception Reason Calculation</label>
        <required>false</required>
