        <fullName>Catagories_less_then_2_picklist_val</fullName>
        <active>true</active>
        <errorConditionFormula>IF(Includes(Categories,&quot;Training&quot;),1,0)+
IF(Includes(Categories,&quot;System Issues&quot;),1,0)+
IF(Includes(Categories,&quot;Support&quot;),1,0)+
IF(Includes(Categories,&quot;Future Enhancements&quot;),1,0)+
IF(includes(Categories,&quot;Reporting&quot;),1,0)&gt;1</errorConditionFormula>
