    <fields>
        <fullName>MPM4_BASE__Actual_Duration_Calc__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF ( ISBLANK(MPM4_BASE__Actual_Finish_Date__c),   MPM4_BASE__Deadline__c  - IF(ISBLANK( MPM4_BASE__Actual_Start_Date__c),   MPM4_BASE__Kickoff__c ,  MPM4_BASE__Actual_Start_Date__c ) + 1, MPM4_BASE__Actual_Finish_Date__c  - IF(ISBLANK( MPM4_BASE__Actual_Start_Date__c),   MPM4_BASE__Kickoff__c ,  MPM4_BASE__Actual_Start_Date__c )+1)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Duration</label>
