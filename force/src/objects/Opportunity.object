    <validationRules>
        <fullName>Amount_Required_ShiftOpp</fullName>
        <active>true</active>
        <description>The Amount cannot be null when a Shift Opportunity is changed to Closed Won.</description>
        <errorConditionFormula>AND( 
$RecordType.Name = &quot;Shift Opportunity&quot;, 
ISPICKVAL(StageName, &quot;Closed Won&quot;), 
ISBLANK( Amount )
)</errorConditionFormula>
        <errorDisplayField>Amount</errorDisplayField>
        <errorMessage>The Amount cannot be null when a Shift Opportunity is changed to Closed Won.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Amount_Value_Timeframe_Required_ShiftOpp</fullName>
        <active>true</active>
        <description>The Amount Value Timeframe cannot be null when a Shift Opportunity is changed to Closed Won.</description>
        <errorConditionFormula>AND( 
$RecordType.Name = &quot;Shift Opportunity&quot;,
ISPICKVAL(StageName, &quot;Closed Won&quot;), 
ISPICKVAL(Amount_Value_Timeframe__c, &quot;&quot; )
)</errorConditionFormula>
        <errorDisplayField>Amount_Value_Timeframe__c</errorDisplayField>
        <errorMessage>The Amount Value Timeframe cannot be null when a Shift Opportunity is changed to Closed Won.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Competitor_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISPICKVAL( Lost_Reason__c , &quot;Competition&quot;),
ISPICKVAL(Lost_Reason__c, &quot;Cancelled&quot;),
ISNULL( Reason__c ))</errorConditionFormula>
        <errorMessage>Please provide the competitors name in the &quot;Competitor&quot; field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lost_reason_required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISPICKVAL(StageName, &quot;Closed Lost&quot;),
ISPICKVAL(Lost_Reason__c, &quot;&quot;))</errorConditionFormula>
        <errorMessage>Closed Lost opportunities require a &quot;Lost Reason&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VSPR16_SHIFT_CLOSED_OPP_NOT_EDITABLE</fullName>
        <active>true</active>
        <errorConditionFormula>$RecordType.Name = &quot;Shift Opportunity&quot;

&amp;&amp; 

ISPICKVAL(PRIORVALUE(StageName), &quot;Closed Won&quot;)

&amp;&amp;

(ISCHANGED(Name) || 
ISCHANGED( AccountId ) || 
ISCHANGED(Line_of_Business__c) || 
ISCHANGED(Division_Brand__c) || 
ISCHANGED(Product_Program__c) || 
ISCHANGED(NextStep) || 
ISCHANGED(Description) || 
ISCHANGED( CampaignId ) || 
ISCHANGED(StageName) ||
ISCHANGED(Probability)|| 
ISCHANGED(CloseDate) || 
ISCHANGED(Amount) || 
ISCHANGED(Amount_Value_Timeframe__c) || 
ISCHANGED(Project_Type__c) || 
ISCHANGED(Lost_Reason__c) || 
ISCHANGED(LeadSource))

&amp;&amp;

NOT(OR( $Profile.Name = &quot;System Administrator&quot; ,$Profile.Name = &quot;VSP Technical User&quot;, $Profile.Name = &quot;VSP LOB Admin User&quot;))</errorConditionFormula>
        <errorMessage>Opportunities in closed/won status are not editable</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VSPR1_ClosedCancelled</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(PRIORVALUE(StageName), &quot;Closed Cancelled&quot;)

&amp;&amp;
 
ISCHANGED(StageName)

&amp;&amp;

NOT(OR($Profile.Name = &quot;VSP LOB Admin User&quot;, $Profile.Name = &quot;System Administrator&quot;, $Profile.Name = &quot;VSP Technical User&quot;, $Profile.Name = &quot;VSP CRM Support User&quot;))

||

ISPICKVAL(PRIORVALUE(StageName), &quot;Closed Cancelled&quot;) 

