    <validationRules>
        <fullName>MPM4_BASE__CannotBeBlockedIfComplete</fullName>
        <active>true</active>
        <errorConditionFormula>if (And ( MPM4_BASE__Blocked__c == true, MPM4_BASE__Complete__c == true), true, false)</errorConditionFormula>
        <errorDisplayField>MPM4_BASE__Blocked__c</errorDisplayField>
        <errorMessage>You cannot mark a completed task as blocked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MPM4_BASE__CannotBeCompleteIfBlocked</fullName>
        <active>true</active>
        <errorConditionFormula>if (and ( MPM4_BASE__Complete__c == true,  MPM4_BASE__Blocked__c ==true), true, false)</errorConditionFormula>
        <errorDisplayField>MPM4_BASE__Complete__c</errorDisplayField>
        <errorMessage>You cannot complete a task marked as blocked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MPM4_BASE__Due_Date_Greater_Than_Milestone_Deadline</fullName>
        <active>false</active>
        <errorConditionFormula>MPM4_BASE__Due_Date__c &gt;  MPM4_BASE__Project_Milestone__r.MPM4_BASE__Deadline__c</errorConditionFormula>
        <errorDisplayField>MPM4_BASE__Due_Date__c</errorDisplayField>
        <errorMessage>Due Date should be smaller than Milestone Deadline</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MPM4_BASE__Due_Date_Smaller_Than_Milestone_Kickoff</fullName>
        <active>false</active>
        <errorConditionFormula>MPM4_BASE__Due_Date__c &lt;  MPM4_BASE__Project_Milestone__r.MPM4_BASE__Kickoff__c</errorConditionFormula>
        <errorDisplayField>MPM4_BASE__Due_Date__c</errorDisplayField>
        <errorMessage>Due Date should be greater than Milestone Kickoff Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MPM4_BASE__Must_be_blank_if_not_blocked</fullName>
        <active>true</active>
        <errorConditionFormula>IF( and(MPM4_BASE__Blocked__c == false, len( MPM4_BASE__Blocked_Reason__c ) &gt; 0) , True, False)</errorConditionFormula>
        <errorDisplayField>MPM4_BASE__Blocked_Reason__c</errorDisplayField>
        <errorMessage>Must be blank if &quot;blocked&quot; is not checked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MPM4_BASE__Must_not_be_blank_if_blocked</fullName>
        <active>true</active>
        <errorConditionFormula>IF( and(MPM4_BASE__Blocked__c == true, len( MPM4_BASE__Blocked_Reason__c ) == 0) , True, False)</errorConditionFormula>
        <errorDisplayField>MPM4_BASE__Blocked_Reason__c</errorDisplayField>
        <errorMessage>If this task is blocked you must enter a reason.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MPM4_BASE__Predecessor_Task_of_Successor_Task</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(ISBLANK(MPM4_BASE__Predecessor_Task__r.MPM4_BASE__Predecessor_Task__c)), Id  =  MPM4_BASE__Predecessor_Task__r.MPM4_BASE__Predecessor_Task__c)</errorConditionFormula>
        <errorDisplayField>MPM4_BASE__Predecessor_Task__c</errorDisplayField>
        <errorMessage>Cannot have a predecessor task that is the successor of the task.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MPM4_BASE__Start_Date_More_Than_Due_Date</fullName>
        <active>false</active>
        <errorConditionFormula>if (and(!isblank(MPM4_BASE__Start_Date__c), !isblank(MPM4_BASE__Due_Date__c)), true, false)</errorConditionFormula>
        <errorDisplayField>MPM4_BASE__Due_Date__c</errorDisplayField>
        <errorMessage>Due Date should be greater than Start Date</errorMessage>
    </validationRules>
