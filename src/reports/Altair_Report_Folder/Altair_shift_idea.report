<?xml version="1.0" encoding="UTF-8"?>
<Report xmlns="http://soap.sforce.com/2006/04/metadata">
    <aggregates>
        <calculatedFormula>Account.Marchon_and_Altair_Net_Sales_LTM__c:SUM + Account.Optics_VSPOne_YTD_revenue__c:SUM</calculatedFormula>
        <datatype>currency</datatype>
        <description>Eyewear and Optics revenue LTM</description>
        <developerName>FORMULA1</developerName>
        <isActive>true</isActive>
        <isCrossBlock>false</isCrossBlock>
        <masterLabel>LOB Revenue LTM</masterLabel>
        <scale>2</scale>
    </aggregates>
    <aggregates>
        <calculatedFormula>Account.Optics_VSPOne_PM_Revenue__c:SUM + Account.Eyewear_Last_Month_Revenue__c:SUM</calculatedFormula>
        <datatype>currency</datatype>
        <description>Eyewear and Optics Last month Revenue</description>
        <developerName>FORMULA2</developerName>
        <isActive>true</isActive>
        <isCrossBlock>false</isCrossBlock>
        <masterLabel>LOB Last Month Revenue</masterLabel>
        <scale>2</scale>
    </aggregates>
    <aggregates>
        <calculatedFormula>((Account.Marchon_and_Altair_Net_Sales_LTM__c:SUM + Account.Optics_VSPOne_YTD_revenue__c:SUM) / 12) - (Opportunity.Amount:SUM / 24)</calculatedFormula>
        <datatype>currency</datatype>
        <description>above or below Amount filed on Opportunity</description>
        <developerName>FORMULA3</developerName>
        <isActive>true</isActive>
        <isCrossBlock>false</isCrossBlock>
        <masterLabel>example of conditional color</masterLabel>
        <scale>2</scale>
    </aggregates>
    <chart>
