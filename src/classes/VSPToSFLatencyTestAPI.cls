@RestResource(urlMapping='/TestLatency/*')
global with sharing class VSPToSFLatencyTestAPI {
    
    @HttpPost
    global static string getPayload(Integer kSize) {
        String output = '';
        if (kSize <= 1000) {
            final string hundredKChars= '""';      
                if (kSize < 10) {
                    if (kSize == 1) {
                        output = hundredKChars.substring(0, 1024);
                    }
                    if (ksize == 5) {
                        output = hundredKChars.substring(0, 5120);
                    }
                } else {
                    Integer iterations = kSize / 10;
                    for (Integer i=0; iterations > i; i++) {
                        output += hundredKChars;
                    }
                }
        }
        return output;
    }
}