<apex:page standardController="Knowledge__kav" cache="false"  id="supportqueuepageid" sidebar="false" renderAs="pdf">

    <apex:pageBlock mode="detail" html-style="font:12px;width:90%;float:left;">
        <apex:pageBlockSection columns="1">
            <apex:outputfield value="{!Knowledge__kav.Title}"/><br/>
            <apex:outputfield value="{!Knowledge__kav.ArticleNumber}"/><br/>
            <apex:outputfield value="{!Knowledge__kav.VersionNumber}"/><br/>
            <apex:outputfield value="{!Knowledge__kav.LastPublishedDate}"/><br/>
            <apex:outputfield value="{!Knowledge__kav.Version_Comments__c}"/><br/>
            <apex:outputLabel value="================================================================================"/><br/><br/>

        <apex:pageBlockSectionItem >
            <apex:outputField label="Article Details" value="{!Knowledge__kav.Article_Details__c}"/>
        </apex:pageBlockSectionItem>

            <apex:outputField label="Keyword" value="{!Knowledge__kav.Keyword__c}"/>    
            <apex:outputField label="SQ/QQ Steps" value="{!Knowledge__kav.Support_Queue_Steps__c}"/>

        </apex:pageBlockSection>
    </apex:pageBlock>    
</apex:page>