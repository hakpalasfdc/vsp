    <validationRules>
        <fullName>VSP_Premier_Active_Rule</fullName>
        <active>true</active>
        <errorConditionFormula>TEXT(Status__c) = &apos;Active&apos;

&amp;&amp; 

NOT(ISCHANGED(Status__c))

&amp;&amp;

OR(
ISCHANGED(Description__c),
ISCHANGED(Effective_Date__c),
ISCHANGED(Exclusive_VSP_Patient_Offers__c),
ISCHANGED(Eyewear_Minimum__c),	
ISCHANGED(Free_Fitting_and_Adjustments__c),  
ISCHANGED(Hierarchy_Action__c),    
ISCHANGED(Hierarchy_Selection__c),
ISCHANGED(Lens_Minimum__c),  
ISCHANGED(Retinal_Imaging_Indicator__c),
ISCHANGED(Name),
ISCHANGED(Total_Purchases_LTM__c),	
ISCHANGED(VSP_Flex_Amount__c),
ISCHANGED(Contract_Type__c),
ISCHANGED(Agreement_Type__c)
)

&amp;&amp;

NOT(OR( 
$Profile.Name = &quot;System Administrator&quot;, 
$Profile.Name = &quot;VSP Technical User&quot;) 
)

&amp;&amp;

NOT(OR(ISCHANGED(Bypass_Validation__c ), 
						Bypass_Validation__c = True))</errorConditionFormula>
