    <fields>
        <fullName>Activities_I_Manage__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
				AND($User.MarketingID__c &lt;&gt; null, CONTAINS(Owner:User.MarketingID__c,$User.MarketingID__c))
				|| 
				AND($User.MarketingID2__c &lt;&gt; null, CONTAINS(Owner:User.MarketingID2__c,$User.MarketingID2__c))
				, 
				&quot;True&quot;, 
				&quot;False&quot;)</formula>
