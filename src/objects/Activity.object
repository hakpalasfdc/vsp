    <listViews>
        <fullName>Julia_s_Tasks</fullName>
        <booleanFilter>1 AND 2 AND (3 or 4)</booleanFilter>
        <columns>TASK.SUBJECT</columns>
        <columns>TASK.CREATED_DATE</columns>
        <columns>EVENT.START_DATE_TIME</columns>
        <columns>TASK.WHO_NAME</columns>
        <columns>TASK.WHAT_NAME</columns>
        <columns>ACTIVITY.TASK</columns>
        <columns>TASK.DUE_DATE</columns>
        <columns>TASK.STATUS</columns>
        <columns>CORE.USERS.ALIAS</columns>
        <columns>TASK.LAST_UPDATE</columns>
        <filterScope>Everything</filterScope>
        <filters>
            <field>CORE.USERS.ALIAS</field>
            <operation>equals</operation>
            <value>jrobi</value>
        </filters>
        <filters>
            <field>TASK.STATUS</field>
            <operation>notEqual</operation>
            <value>Completed</value>
        </filters>
        <filters>
            <field>ACTIVITY.TASK</field>
            <operation>equals</operation>
            <value>1</value>
        </filters>
        <filters>
            <field>EVENT.END_DATE_TIME</field>
            <operation>greaterThan</operation>
            <value>TODAY</value>
        </filters>
        <label>Julia&apos;s Tasks</label>
        <sharedTo>
            <group>LOB_Admin_Group</group>
            <role>GBD_Manager_Sales_Operations</role>
            <role>GBD_Sales_Operations_Specialists</role>
        </sharedTo>
    </listViews>
